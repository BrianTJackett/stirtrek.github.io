<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <%
        var title = page.title ? page.title + ' - ' + config.title : config.title
    %>

    <title><%= title %></title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Twitter Card data -->
    <meta name="twitter:card" value="summary">
    <meta name="twitter:site" value="<%= config.twitterHandle %>">
    <meta name="twitter:title" value="<%= title %>">
    <meta name="twitter:description" value="<%= config.description %>">
    <meta name="twitter:image" value="<%= config.socialMediaImage %>">

    <!-- Open Graph data -->
    <meta property="og:title" content="<%= title %>" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="<%= config.url %>" />
    <meta property="og:image" content="<%= config.socialMediaImage %>" />
    <meta property="og:description" content="<%= config.description %>" />


    <link rel="icon" href="/icons/favicon-114.png" sizes="114x114">
    <link rel="icon" href="/icons/favicon-72.png" sizes="72x72">

    <link href="/styles/bootstrap.min.css" rel="stylesheet">
    <link href="/styles/bootstrap-button-social.min.css" rel="stylesheet">
    <link href="/styles/fontawesome-4.min.css" rel="stylesheet">


    <script src="https://code.jquery.com/jquery-3.3.1.js" integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="  crossorigin="anonymous"></script>
    <!-- <script src="/scripts/bootstrap.js"></script> -->
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <link href="/styles/site.css" rel="stylesheet">

    <script>
    if ('serviceWorker' in navigator) {
        navigator.serviceWorker.register('/serviceworker.js').then(function(registration) {
        // Registration was successful
        console.log('ServiceWorker registration successful with scope: ', registration.scope);
    }).catch(function(err) {
        // registration failed :(
            console.log('ServiceWorker registration failed: ', err);
        });
    }
    </script>

</head>