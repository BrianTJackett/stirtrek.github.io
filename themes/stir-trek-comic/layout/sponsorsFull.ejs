<!-- Sponsors -->
<div id="allSponsorDetail">
    <section>
        <div class="row">
            <% for (var sponsor of site.data.sponsors.principal ) {%>

                <div class="col-md-6 sponsor sponsor-<%= sponsor.level %>">
                    <div class="comic-panel-body">
                        <a href="sponsor.link">
                            <img class="img-responsive" src="<%= sponsor.logo %>" title="<%= sponsor.description %>">
                        </a>
                    </div>
                    <div class="sponsorsAbout text-with-comic-panel">
                        <div class="sponsorCompany">
                            <%= sponsor.name %>
                            <br>
                            <a href="<%= sponsor.link %>"><%= sponsor.link %></a>
                            <div class="description">
                                <%= sponsor.description %>
                            </div>
                        </div>
                    </div>
                </div>
                
            <% } %>
        </div>
    </section>
    <section>
        <div class="row">

            <% for (var sponsor of site.data.sponsors.platinum ) {%>

                <div class="col-md-4 sponsor sponsor-<%= sponsor.level %>">
                    <div class="comic-panel-body">
                        <a href="sponsor.link">
                            <img class="img-responsive" src="<%= sponsor.logo %>" title="<%= sponsor.description %>">
                        </a>
                    </div>
                    <div class="sponsorsAbout text-with-comic-panel">
                        <div class="sponsorCompany">
                            <%= sponsor.name %>
                            <br>
                            <a href="<%= sponsor.link %>"><%= sponsor.link %></a>
                            <div class="description">
                                <%= sponsor.description %>
                            </div>                            
                        </div>
                    </div>
                </div>
                
            <% } %>
        </div>
    </section>
</div>